<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/gear"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.SemyungPage"
        tools:targetApi="31"
        android:usesCleartextTraffic="true">


        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".NoticeboardActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".AnonyActivity"></activity>
        <activity android:name=".SeePostActivity"></activity>
        <activity android:name=".AddComments" ></activity>
        <activity android:name=".AddPostActivity" ></activity>
        <activity android:name=".SearchActivity" ></activity>


    </application>
    <uses-permission android:name="android.permission.INTERNET" />


</manifest>